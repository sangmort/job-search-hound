{
  "manifest_version": 3,
  "name": "JobSearch Hound",
  "version": "1.0.0",4
  "description": "Easier way to search LinkedIn & Indeed.",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "fun.css"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start",
      "match_about_blank": true,
      "all_frames": true
    }
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {},
    "default_title": "Job Search Hound",
    "default_area": "navbar"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs"
  ]
}